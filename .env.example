# DDGS initialization parameters
# Proxy settings (optional), format: http://proxy.example.com:8080 or socks5://proxy.example.com:1080
DDGS_PROXY=

# Request timeout (seconds), default 10 seconds
DDGS_TIMEOUT=10

# Default search parameters
# Default search region, e.g.: us-en(US English), wt-wt(Worldwide), zh-cn(China)
DEFAULT_REGION=wt-wt

# Default safe search level: off, moderate, strict
DEFAULT_SAFESEARCH=moderate

# Default time limit (optional): d(day), w(week), m(month), y(year)
DEFAULT_TIMELIMIT=

# Default maximum results (1-100)
DEFAULT_MAX_RESULTS=10

# Default page number
DEFAULT_PAGE=1

# Default search backend: auto, html, lite
DEFAULT_BACKEND=auto

# API Key list
API_KEYS=

# MCP
MCP_ENABLE_TOOLS=["text"]
